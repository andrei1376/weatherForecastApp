    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  
  end

  target 'weatherForecastUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Quick'
    pod 'Nimble'
  end

end
